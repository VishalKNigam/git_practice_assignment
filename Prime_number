    var num=13;
    var count = 2;
    if (num==1){
        console.log("No");
    }else if(num>=2){
        for(var i=2; i<num; i++){
            if(num%i==0){
                count++;
            }
            
            }
            }
            if(count==2){
                console.log("No");
            }else{
                console.log("Yes")
            }
            
        
